<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
	android:layout_height="match_parent" android:orientation="vertical" android:padding="8dp">

	<Button android:id="@+id/createNotification" android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:text="@string/createNotification" />

	<EditText android:id="@+id/url" android:layout_width="match_parent" android:layout_height="wrap_content"/>

	<Button android:id="@+id/createNotificationFromBundle" android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:text="@string/createNotificationFromBundle" />

	<Button android:id="@+id/cancelNotifications" android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:text="@string/cancelNotifications" />

</LinearLayout>
